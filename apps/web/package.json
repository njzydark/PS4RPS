    "@sentry/tracing": "^7.16.0",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "common": "workspace:^1.0.0-beta.7",
    "dayjs": "^1.10.7",
    "json5": "^2.2.1",
    "nanoid": "^3.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.10",
