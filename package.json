{
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=14",
    "pnpm": "6"
  },
  "scripts": {
    "ci:build": "pnpm install && pnpm run desktop:build && pnpm run copyRenderer --filter desktop",
    "desktop:dev": "turbo run dev --filter=desktop --filter=web",
    "desktop:build": "turbo run build --filter=desktop --filter=web",
    "desktop:start": "pnpm run start --filter desktop",
    "desktop:pack": "pnpm run pack --filter desktop",
    "desktop:dist": "pnpm run dist --filter desktop",
    "web:dev": "turbo run dev --filter=web",
    "web:build": "turbo run build --filter=web",
    "preinstall": "npx only-allow pnpm"
  },
  "author": "njzy <njzydark@gmail.com>",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix",
    "*.{ts,tsx,html,css,less,md}": "prettier --write"
  },
